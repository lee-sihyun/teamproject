                        console.log('optName : ' + optName);
                        console.log('optQuantity : ' + optQuantity);
                        console.log('});');
                        //allCnt -= Number(optQuantity);
                    }
                });
            } else {
                allCnt = 1;
            }
            console.log('modeStr : ' + modeStr + ' / allCnt : ' + allCnt);
            if (modeStr == 'w') {
