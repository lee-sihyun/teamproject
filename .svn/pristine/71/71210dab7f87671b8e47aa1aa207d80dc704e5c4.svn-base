.all_menu{display:none; position:absolute;top:100px;left:0;width:100%;padding:50px 0 30px;background: #333;text-align:left;z-index:100;}
.all_menu:after{content:'';display:block;  position:absolute;top:-20px;left:50%;margin-left:-190px;width:0;height:0;border-top:10px solid transparent;border-bottom:10px solid #333;border-left:10px solid transparent;border-right:10px solid transparent; }
.all_menu.cate_over{display:block;}
.all_menu .all-category-layer{  margin:0 auto; }
.all_menu .all-category-layer > div > ul > li > strong{display:none;}
.all_menu .all-category-layer > div > ul > li > ul{display:table; margin:0 auto;  width:1200px;}
.all_menu .all-category-layer > div > ul > li > ul > li{display:table-cell;vertical-align:top;padding:0 15px;}
.all_menu .all-category-layer > div > ul > li > ul > li > a{color:#fff;font-size:18px; font-family : "notokrB";} 
.all_menu .all-category-layer > div > ul > li > ul > li > ul{}
.all_menu .all-category-layer > div > ul > li > ul > li > ul > li{padding-top:20px;font-size:14px;letter-spacing:-1px;}
.all_menu .all-category-layer > div > ul > li > ul > li > ul > li > a{color:#9b9b9b;}
.all_menu .all-category-layer > div > ul > li > ul > li > ul > li > a:hover{color:#fff;}
.all_menu .all-category-layer > div > ul > li:nth-child(2){display:none;}
.all_menu .all-category-layer > div > ul > li:nth-child(3){display:none;}
.all_menu button{display:none;}

.all_brand{display:none; position:absolute;top:100px;left:0;width:100%;padding:50px 0 50px;background: #333;text-align:left;z-index:100;}
