
/********************
 카테고리 상품리스트
********************/
.location { display: block; position:relative; padding: 20px 0 0; z-index:1; }
.location .path{ display:block; float:right; width:620px;}
.location .path span{float:left; line-height:20px;}

.cg-main > h3 { display:block; float:left; width:100%; padding: 30px 0; color: #000; font-size: 11px; text-align: center; }
.cg-main .weekly-best { margin: 0 auto; padding: 0 0; border: 0; }

.cg-main{display:block; max-width:1180px; margin:0 auto;}
.cg-main > h2{display:block; text-align:left; padding: 50px 0 10px 5px; color: #111; font-size: 16px; letter-spacing:2px; border-bottom: 1px solid #dddddd; }

.cg-main .lower-category {padding: 10px 0; border: none; border-top: none; font-size: 0; text-align:left; }
.cg-main .lower-category li { display: inline-block; width: auto; padding:0; text-align: center; vertical-align: middle; }
.cg-main .lower-category li span { display: block;   padding:0 16px; margin:0;font-size: 14px; }
.cg-main .lower-category li:first-child span{padding-left:0;}
.cg-main .lower-category li span a { display: inline-block; padding:10px 0; font-size: 14px; color: #8f8f8f; }
.cg-main .lower-category li span a img { max-width: 100%; max-height: 20px; }
.cg-main .lower-category li span:hover {  color:#000; }
.cg-main .lower-category li span:hover a{ color:#000; text-decoration:none;}
.cg-main .lower-category li span.on{  color:#000; }
.cg-main .lower-category li span.on a{ color:#000; }
.cg-main .lower-category li span em { }
.cg-main .lower-category li span:hover em,
.cg-main .lower-category li span.on em{ color:#000;  }

.view .goods-list { display:block; float:left; width:100%; position: relative; padding: 40px 0 0; border-bottom: none; }
.view .goods-list .prdlink{display:block; float:left; width:100%; margin:0 auto; border-bottom: none; }
.view .goods-list .prdlink h2{display:inline-block; float:left; color:#333333; font-size:24px; font-weight:bold; line-height:48px;}
.view .goods-list .prdlink form{display:inline-block; float:right;}
.view .goods-list .tc { display:inline-block; float:left; line-height:23px; margin:20px 0 0; color: #303030; font-size: 11px; }
.view .goods-list .tc strong { /*border-radius:5px; color: #FFFFFF;*/ padding:5px 15px; font-size:11px; /*text-decoration:underline; background:#666666;*/ }

.view .goods-list fieldset{display:inline-block; float:right;}
.view .goods-list .align { display:inline-block; float:right; height: 100%; margin: 10px 0; padding: 0; border-top: none; border-bottom: none; }
.view .goods-list .align ul { float: left; }
.view .goods-list .align ul li { position: relative; float: left; height: auto; margin: 0 14px 0 0; padding: 5px 0 0; background: #fff; }
.view .goods-list .align .chosen-container .chosen-results { float: none; }
.view .goods-list .align .chosen-container .chosen-results .active-result { float: none; height: auto; margin: 0; padding: 0; }
.view .goods-list .align .chosen-container .chosen-results .highlighted { background: #e9e9e9; }

.view .goods-list .align ul li .radio { position: absolute; bottom: 0; left: 0; z-index: -1; }
.view .goods-list .align ul li label { position:relative; padding: 0 0 0 0; background: #fff; cursor: pointer; font-size:11px; letter-spacing:-1px; }
.view .goods-list .align ul li label.on { background: none; color: #333333; border-bottom:1px solid #333333;  }
.view .goods-list .align .choice { float: right; margin-bottom:0px; }

.view .goods-list .item-display{ display:block; float:left; width:100%; margin-top:30px;}


.order-goods { background: #f5f5f5; border-top: none; border-bottom: 2px solid #666666; }
.goods-view .goods { position:relative; z-index:0; }
.goods-view .goods .image .thumbnail { display: table-cell; text-align: center; vertical-align: middle; }
.goods-view .goods .info .item ul li.price > div strong { color: #666666; font-family: arial; font-size: 16px; }
.goods-view .goods .info .end-price ul .total strong { font-size: 24px; color: #000000; }

.skinbtn.point1 { border: 1px solid #000000; color: #000000; }
.skinbtn.point2 { background: #ffffff; border: 1px solid #000000; color: #000000;  }
.skinbtn.point2.btn-add-order { width: 185px; height: 50px; line-height: 50px; font-size: 14px; font-weight: bold; }
.skinbtn.point1.btn-add-cart, .skinbtn.point1.btn-add-wish { width: 184px; height: 50px; line-height: 50px; font-size: 14px; font-weight: normal; }
.skinbtn.point2.btn-add-cart { margin:0 4px; }

.skinbtn.point2:hover { background: #000000; border: 1px solid #000000; color:#FFFFFF; }
.skinbtn.point1:hover { background: #FFFFFF; border: 1px solid #000000; color:#000000; text-decoration:underline; }


.skinbtn { display: inline-block; *display: inline; *zoom: 1; height: 40px; padding: 0 5px; text-align: center; vertical-align: top; box-sizing: border-box; cursor: pointer; font-size: 12px; }
.goods-view .goods .info .btn > * { margin: 0 2px;}
.goods-view .goods .info .btn > * + * { margin: 0 2px;}