    right: 0;
    background: url('../img/btn/gv-thumbnail-next.png') no-repeat left top;
}

.goods-view .goods .image .more-thumbnail .prev:hover {
    left: 0;
   /*  background: url('../img/btn/gv-thumbnail-prev-ov.png') no-repeat left top; */
}

