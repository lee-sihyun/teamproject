    right: 0;
    /* background: url('../img/btn/gv-thumbnail-next-ov.png') no-repeat left top; */
}

.goods-view .goods .image .more-thumbnail .prev a , .goods-view .goods .image .more-thumbnail .next a {
    display:block;
    width: 7px;
    height: 13px;
    text-indent:-9999px;
