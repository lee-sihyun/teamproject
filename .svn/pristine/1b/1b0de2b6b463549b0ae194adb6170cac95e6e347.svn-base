<%@page import="site.itwill.dao.ProductDAO"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%
	request.setCharacterEncoding("UTF-8");

	String[] checkNum = request.getParameterValues("checkNum");

	for (String num:checkNum) {

		ProductDAO.getDAO().removeProduct(Integer.parseInt(num));

	}
	response.sendRedirect(request.getContextPath()+"/index.jsp?workgroup=product&work=product_list");
%>
