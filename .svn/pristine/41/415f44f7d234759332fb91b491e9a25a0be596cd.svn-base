/*
    센터정렬 레이어
*/
.layer-wrap {
    position:fixed;
    left:0;
    top:0;
    z-index:100;/*플러스리뷰*/ /* z-index 증가시 결제 레이어팝업이 pg팝업보다 위에 뜨므로 주의 */
    overflow-y:auto;
    width:100%;
    height:100%;

	background:rgba(0,0,0,0.3);
}
.layer-wrap .box {
    background:#fff;
    /* border:2px solid #555; */
}
.layer-wrap .box .view {
    position:relative;
    margin:28px 20px;
}
.layer-wrap .box .view h2 {
    padding:0 0 25px;
    border-bottom:1px solid #000;
    color:#000;
    font-size:22px;
	font-family : "Noto Sans KR";
	font-weight:normal;
	letter-spacing:1px;

}
.layer-wrap .box .view h2.none-bd {
    border:none;
}
.layer-wrap .box .view .tit {
    overflow:hidden;
    padding:20px 10px 10px;
}
.layer-wrap .box .view .tit h3 {
    float:left;
    padding:6px 0 0;
}
.layer-wrap .box .view .tit .skinbtn {
    float:right;
}
.layer-wrap .box .view .table1 {
    padding:0 10px;
}
.layer-wrap .box .view > .btn,
.layer-wrap .box .view > form > .btn {
    padding:20px 0 0; 
    text-align:center;
}
.layer-wrap .box .view > .btn.top-line,
.layer-wrap .box .view > form > .btn.top-line {
    margin:20px 0 0;
    border-top:1px solid #e6e6e6;
}
.layer-wrap .box .view .close {
    display:block;
    position:absolute;
    top:6px;
    right:0;
    width:18px;
    height:18px;
    text-indent:-9999px;
    background:url('../img/btn/layer-close.png') no-repeat left top;
}
.layer-wrap .box .layer-cart-btn {
    
}
.layer-wrap .scroll-box  {
    overflow-x:hidden;
    overflow-y:auto;
}

/*
    고정좌표 레이어
*/
.fix-layer-wrap {
    position:absolute;
    background:#fff;
    border:1px solid #555;
}
.fix-layer-wrap .box {
    position:relative;
}
.fix-layer-wrap .box > strong {
    display:block;
    height:27px;
    padding:13px 0 0 17px;
    background:#f4f4f4;
    color:#222;
}
.fix-layer-wrap .box > .view {
    padding:20px;
}
.fix-layer-wrap .box .close {
    display:block;
    position:absolute;
    top:11px;
    right:12px;
    width:18px;
    height:18px;
    text-indent:-9999px;
    background:url('../img/btn/layer-close.png') no-repeat left top;
}
/*
    팁 레이어
*/
.tip-layer {
