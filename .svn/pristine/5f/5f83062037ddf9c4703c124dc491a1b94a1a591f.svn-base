<%@page import="site.itwill.dto.LushUserDTO"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%
    //세션으로 공유된 인증정보(회원정보)를 반환받아 저장
    LushUserDTO loginMember = (LushUserDTO)session.getAttribute("loginMember");

    if(loginMember==null || loginMember.getStatus()!=9) {
        session.setAttribute("message", "로그인 상태의 관리자만 접근 가능합니다.");
        out.println("<script>");
        out.println("location.href='"+request.getContextPath()+"/index.jsp?workgroup=lushuser&work=login'");
        out.println("</script>");
        return;
    }
%>