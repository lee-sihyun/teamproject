.tooltip_btn{    display: inline-block;
    margin: 0;
    width: 14px;
    height: 14px;
    vertical-align: middle;
    background: url('/admin/gd_share/img/icon_tooltip_off.png') no-repeat;
    cursor: pointer;
	position:relative;
	}
.tooltip_btn .tooltip_con{display:none;}
.tooltip_btn:hover .tooltip_con{display:block;position:absolute;top:-10px;left:30px;width:250px;border:1px solid #ccc;background: #fff; z-index:1;}
.tooltip_btn:hover .tooltip_con:before{  content:''; display:inline-block;
  position: absolute;
  width: 0;
  height: 0;
  border-color: transparent;
  border-style: solid;
  top: 10px;
  left: -8px;
  margin-top: 0;
  border-width: 8px 8px 8px 0;
  border-right-color: #ccc;}
.tooltip_btn:hover .tooltip_con:after{  content:''; display:inline-block;
  position: absolute;
  width: 0;
  height: 0;
  border-color: transparent;
  border-style: solid;
  top: 10px;
  left: -6px;
  margin-top: 0;
  border-width: 8px 8px 8px 0;
  border-right-color: #fff;}
.tooltip_btn .tooltip_con > span{display:block;padding:8px;font-size:12px;color:#555;}

.toggle_w{position:relative;}
.toggle_btn{ display: inline-block; margin: 0; width: 14px; height: 14px; vertical-align: middle; background: url('/admin/gd_share/img/icon_tooltip_off.png') no-repeat; cursor: pointer; position:relative;}
.toggle_content{display:none; position:absolute;top:-5px;left:20px; width:575px;border:1px solid #e7e7e7;background: #fff; z-index:1;}
.toggle_content .inner{padding:10px 30px 10px 10px;font-size:12px;color:#808080;}
.toggle_content .toggle_close{position:absolute;top:10px;right:10px;width:19px;height:18px;background: url('../../img/btn/layer-close.png') no-repeat left top;font-size:0;}

