}

tbody {
	width: 1663px;
	height: 59px;
	isplay: table-row-group;
	vertical-align: middle;
	border-color: inherit;
}

.btnCtrl {
	width: 52px;
	height: 28px;
	/* padding: 0 10px 0 10px;*/
	margin: 0 5px 0 5px;
	color: #fff;
	font-size: 13px;
	font-weight: normal;
	line-height: 28px;
	background-color: #7c7e8c;
	transition: .2s ease-out;
	display: inline-block;
	position: relative;
	text-align: center;
	vertical-align: middle;
	text-decoration: none;
	white-space: nowrap;
	box-sizing: border-box;
	color: #fff;
}

